<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="PhotomBundle\Entity\Pregunta" table="Pregunta">
    <id name="idpregunta" type="integer" column="idPregunta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="preguntauno" type="string" column="preguntaUno" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="preguntados" type="string" column="preguntaDos" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="preguntatres" type="string" column="preguntaTres" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
